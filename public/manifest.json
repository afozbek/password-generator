{
  "name": "My Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Password Generator",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "./options.html",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./content.js"]
    }
  ]
}
